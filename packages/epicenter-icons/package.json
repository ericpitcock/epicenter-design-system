{
  "name": "@ericpitcock/epicenter-icons",
  "version": "2.0.0-beta",
  "description": "Epicenter Icons, A comprehensive Vue 3 icon library",
  "type": "module",
  "main": "./index.js",
  "module": "./index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js"
    },
    "./base.scss": "./base.scss",
    "./icons/*": {
      "import": "./epicenter-icons/*.js",
      "require": "./epicenter-icons/*.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "epicenter-icons/",
    "index.js",
    "base.scss",
    "README.md"
  ],
  "scripts": {
    "download": "python3 scripts/download_svgs.py",
    "clean": "python3 scripts/clean_svgs.py",
    "generate": "python3 scripts/generate_vue_components.py",
    "generate-index": "node scripts/generate-index.mjs",
    "build": "npm run generate && npm run generate-index",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "vue",
    "vue3",
    "icons",
    "svg",
    "components",
    "epicenter",
    "ui"
  ],
  "author": "Eric Pitcock",
  "license": "MIT"
}